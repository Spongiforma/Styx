    "express-handlebars": "^5.3.2",
    "handlebars": "^4.7.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^5.3.1",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.3.1",
    "@types/shelljs": "^0.8.9",
    "fs-extra": "^10.0.0",
    "nodemon": "^2.0.11",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^10.1.0",
